{
  "name": "Immersve",
  "license": "MIT",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "clean": "rm -rf {dist,client}/*",
    "compile-contracts": "tealscript src/*.algo.ts dist/",
    "generate-clients": "algokit generate client dist/ --language typescript  --output client/{contract_name}Client.ts",
    "build": "yarn run clean && yarn run compile-contracts && yarn run generate-clients",
    "test": "yarn run build && yarn test __test__/Immersive.test.ts --timeout 60000"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@algorandfoundation/algokit-client-generator": "^3.0.1",
    "@algorandfoundation/algokit-subscriber": "^1.2.0-beta.2",
    "@algorandfoundation/algokit-utils": "^5.8.0",
    "@algorandfoundation/tealscript": "^0.90.0",
    "@types/websocket": "^1.0.10",
    "algo-msgpack-with-bigint": "^2.1.1",
    "algosdk": "^2.7.0",
    "ts-jest": "^29.1.2",
    "websocket": "^1.0.34"
  }
}
