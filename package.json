{
  "name": "Immersve Concept",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "clean": "rm -rf {dist,client}/*",
    "compile-contracts": "tealscript src/*.algo.ts dist/",
    "generate-clients": "algokit generate client dist/ --language typescript  --output client/{contract_name}Client.ts",
    "build": "bun run clean && bun run compile-contracts && bun run generate-clients",
    "test": "bun run build && bun test __test__/Immersive.test.ts --timeout 60000",
    "test:bugged": "bun run build:bugged && bun test __test__/BuggedImmersive.test.ts"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@algorandfoundation/algokit-client-generator": "^3.0.0",
    "@algorandfoundation/algokit-utils": "^5.5.0",
    "@algorandfoundation/tealscript": "^0.86.0",
    "@types/websocket": "^1.0.10",
    "algo-msgpack-with-bigint": "^2.1.1",
    "algosdk": "^2.7.0",
    "websocket": "^1.0.34"
  }
}