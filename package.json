{
  "name": "Immersve Concept",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "compile": "tealscript src/Immersve.algo.ts dist/",
    "generate-client": "algokit generate client dist/ --language typescript --output client/{contract_name}.client.ts",
    "build": "bun run compile && bun run generate-client",
    "test": "bun run build && bun test"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "jest": "^29.7.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@algorandfoundation/algokit-client-generator": "^2.6.0",
    "@algorandfoundation/tealscript": "latest"
  }
}